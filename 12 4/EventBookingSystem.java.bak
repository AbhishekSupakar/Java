import java.util.Scanner;
class Concert{
	String eventName ;
	double ticketPrice ;
	int attendees ;
	public void displayDetails(){
	System.out.println("Concert: "+eventName+" | Ticket Price: "+ticketPrice+" | Number of Attendees: "+attendees);	
	}
}
class TheaterPlay{
